<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="line">
            <stroke android:width="3dp" android:color="@color/radioHyrule_darkGray" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="line">
                <stroke android:width="3dp" android:color="@color/radioHyrule_blueDarker" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress" >
        <clip>
            <shape android:shape="line">
                <stroke android:width="3dp" android:color="@color/radioHyrule_blue" />
            </shape>
        </clip>
    </item>

</layer-list>